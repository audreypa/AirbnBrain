<%= simple_form_for([@brain, @booking]) do |f| %>
  <%= f.input :start_date, as: :string, input_html: { data: { controller: "datepicker", date: @booking_date.to_json , brain: @brain.id.to_json, currentUser: @current_user.id.to_json} } %>
  <%= f.input :end_date, as: :string, input_html: { data: { controller: "datepicker", date: @booking_date.to_json  , brain: @brain.id.to_json, currentUser: @current_user.id.to_json}} %>
  <%= f.submit "Validate"%>
<% end %>
